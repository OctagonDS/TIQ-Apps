{
  "expo": {
    "name": "Trader IQ",
    "slug": "TIQ-Apps",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "icon": "./src/assets/img/iconapp.png",
    "splash": {
      "image": "./src/assets/img/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "translucent": true,
      "backgroundColor": "#00000000"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/img/not-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "notification": { "icon": "./src/assets/img/not-icon.png" },
    "orientation": "portrait",
    "ios": {
      "bundleIdentifier": "octagonds.tiqapp",
      "buildNumber": "1.0.0",
      "icon": "./src/assets/img/iconapp.png"
    },
    "android": {
      "package": "octagonds.tiqapp",
      "versionCode": 1,
      "icon": "./src/assets/img/iconapp-android.png"
    }
  },
  "name": "TIQ-Apps"
}
